package SomandoElementosDeUmArray;

public class SomandoElementosDeUmArray {

	public static void main(String[] args) {
		
		int[] array = { 87, 68, 94, 100, 83, 78, 85, 91, 76, 87 };
		int total = 0;

		somandoElementosDeUmArray(array, total);

	}

	public static void somandoElementosDeUmArray( int paramArray, int paramTotal ) {
		
		
		for ( int counter = 0; counter < paramArray.length; counter++ ) {
			total += paramArray[counter];
		}
		
		System.out.printf("Total o valor de cada elemento %d%n", total);
	}

}
