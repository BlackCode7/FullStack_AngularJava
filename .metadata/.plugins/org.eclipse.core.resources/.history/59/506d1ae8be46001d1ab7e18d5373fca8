package Matrizes;

public class Matrizes {

	public static void main(String[] args) {
	
		/* Aqui é um array */
		int notasArray[] = new int[1];
		
		/* Aqui é uma matriz */
		int notas[][] = new int[1][3];
		
		notas[0][0] = 80;
		notas[0][1] = 90;
		notas[0][2] = 95;
		
		notas[1][0] = 80;
		notas[1][1] = 90;
		notas[1][2] = 95;
		
		System.out.println(notas);
		
	}
	
}
